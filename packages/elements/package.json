{
	"name": "@koroflow/elements",
	"version": "0.0.1-beta.8",
	"type": "module",
	"sideEffects": false,
	"exports": {
		"./globals.css": "./dist/globals.css",
		"./consent-manager-dialog": {
			"types": "./dist/react/consent-manager-dialog/index.d.ts",
			"import": "./dist/react/consent-manager-dialog/index.js",
			"require": "./dist/react/consent-manager-dialog/index.cjs"
		},
		"./consent-manager-widget": {
			"types": "./dist/react/consent-manager-widget/index.d.ts",
			"import": "./dist/react/consent-manager-widget/index.js",
			"require": "./dist/react/consent-manager-widget/index.cjs"
		},
		"./cookie-banner": {
			"types": "./dist/react/cookie-banner/index.d.ts",
			"import": "./dist/react/cookie-banner/index.js",
			"require": "./dist/react/cookie-banner/index.cjs"
		},
		"./common": {
			"types": "./dist/react/common/index.d.ts",
			"import": "./dist/react/common/index.js",
			"require": "./dist/react/common/index.cjs"
		},
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": ["dist"],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "rslib build",
		"check-types": "tsc --noEmit",
		"fmt": "pnpm biome format --write .",
		"lint": "pnpm biome lint ./src",
		"dev": "rslib build --watch"
	},
	"browserslist": [">0.2%", "not dead", "not op_mini all"],
	"peerDependencies": {
		"react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
		"react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@koroflow/typescript-config": "workspace:*"
	},
	"dependencies": {
		"@koroflow/core-js": "0.0.1-beta.8",
		"@radix-ui/react-accordion": "1.2.2",
		"@radix-ui/react-slot": "1.1.1",
		"@radix-ui/react-switch": "1.1.2",
		"clsx": "2.1.1",
		"motion": "^12.0.6",
		"tailwind-variants": "^0.3.1",
		"zustand": "^5.0.3"
	}
}
