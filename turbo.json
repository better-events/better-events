{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"type-check": {
			"outputs": []
		},
		"ci:version": {
			"outputs": []
		},
		"ci:publish": {
			"dependsOn": ["build"],
			"outputs": []
		},
		"//#fmt": {},
		"//#fmt:fix": {
			"cache": false
		}
	}
}
